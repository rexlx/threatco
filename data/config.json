{
    "server_id": "12345",
    "first_user_mode": true,
    "fqdn": "example.com",
    "services": [
        {
            "name": "misp",
            "url": "https://192.168.86.91:443",
            "rate_limited": false,
            "auth_type": "token",
            "key": "Authorization",
            "kind": "service",
            "type": ["md5", "sha1", "sha256", "sha512", "ipv4", "ipv6", "email", "url", "domain", "filepath", "filename"]
        },
        {
            "name": "virustotal",
            "url": "https://www.virustotal.com/api/v3",
            "rate_limited": false,
            "auth_type": "header",
            "key": "Authorization",
            "kind": "service",
            "type": ["md5", "sha1", "sha256", "sha512", "ipv4", "ipv6", "email", "url", "domain", "filepath", "filename"],
            "route_map": [{
                "type": "md5",
                "route": "files"
            }, {
                "type": "sha1",
                "route": "files"
            }, {
                "type": "sha256",
                "route": "files"
            }, {
                "type": "sha512",
                "route": "files"
            }, {
                "type": "ipv4",
                "route": "ip_addresses"
            }, {
                "type": "ipv6",
                "route": "ip_addresses"
            },{
                "type": "url",
                "route": "urls"
            }, {
                "type": "domain",
                "route": "domains"
            }, {
                "type": "filepath",
                "route": "files"
            }, {
                "type": "filename",
                "route": "files"
            }]
        }
    ],
    "http_port": "8080",
    "https_port": "8443",
    "http_too": true,
    "tls_cert": "/path/to/tls.cert",
    "tls_key": "/path/to/tls.key",
    "cert_auth": "/path/to/cert.auth",
    "db_location": "/path/to/db",
    "session_token_ttl": 3600,
    "response_cache_expiry": 600,
    "stat_cache_tick_rate": 60
}